{
    "cluster": {
        "containers": [
            {
                "image": "mysql",
                "tag": "latest",
                "name": "some-mysql-upgrade",
                "environment": [
                    "MYSQL_ROOT_PASSWORD=mysecretpassword"
                ],
                "volumes": [
                    "/var/lib/mysql:/var/lib/mysql"
                ]
            },
            {
                "image": "wordpress",
                "tag": "3.9.2",
                "portToProxy": 8081,
                "name": "some-wordpress-upgrade",
                "links": [
                    "some-mysql:mysql"
                ],
                "portBindings": {
                    "80/tcp": [
                        {
                            "HostIp": "0.0.0.0",
                            "HostPort": "8081"
                        }
                    ]
                }
            }
        ],
        "version": "3.9.2"
    }
}
